<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="ConsentDialogTitleBarBackground">#33affc</color>
    <color name="ConsentDialogTitleBarText">#ffffff</color>
    <color name="ConsentDialogToggleBackground">#00dcdcdc</color>
    <color name="ConsentDialogToggleTitleColor">#323232</color>
    <color name="ConsentDialogUninteractableToggle">#C8C8C8</color>
    <style name="Theme.ConsentDialog" parent="@style/Theme.AppCompat.Light.NoActionBar">
    </style>
    <style name="Theme.ConsentDialogBodyText" parent="@style/TextAppearance.AppCompat.Medium">
        <item name="android:textColor">@android:color/primary_text_light</item>
    </style>
    <style name="Theme.ConsentDialogPhone" parent="@style/Theme.AppCompat.Light.NoActionBar">
    </style>
    <style name="Theme.ConsentDialogSwitch" parent="Theme.AppCompat.Light">
        <!-- active thumb & track color (30% transparency) -->
        <item name="colorControlActivated">#46bdbf</item>

        <!-- inactive thumb color -->
        <item name="colorSwitchThumbNormal">#f1f1f1
        </item>

        <!-- inactive track color (30% transparency) -->
        <item name="android:colorForeground">#42221f1f
        </item>
    </style>
    <style name="Theme.ConsentDialogTablet" parent="@style/Theme.AppCompat.Light.NoActionBar">
    </style>
</resources>